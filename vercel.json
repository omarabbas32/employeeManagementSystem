{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
 
    { "src": "/public/(.*)", "dest": "/public/$1" },
    { "src": "/favicon.ico", "dest": "/public/favicon.ico" },
    { "src": "/admin/assets/(.*)", "dest": "/public/admin/assets/$1" },

    
    { "src": "/(.*)", "dest": "/app.js" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}

